node {
        stage('Clone') {
                git branch: 'feature/2025_03_14', url: 'git@github.com:Srikavya-1/spring-petclinic.git'
			}
			
			 stage('Build') {
        
                sh 'mvn package'
        
			}
			stage('Test') {
        
                sh 'mvn test'
        
			}
        }